<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.frame" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Divulga BR</name>
    <description>
        Divulga BR - sua busca empresarial
    </description>
    <author email="higor533@gmail.com" href="https://higorpedroso.com.br">
        Higor Pedroso Arruda
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="http://*" />
    <access origin="https://*" />
    <allow-intent href="http://*/*" launch-external="yes" />
    <allow-intent href="https://*/*" launch-external="yes" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
